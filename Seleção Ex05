package main

import "fmt"

func main() {
	var n1 int
	fmt.Print("Informe um número: ")
	fmt.Scan(&n1)

	m3 := n1 % 3
	m5 := n1 % 5

	if m3 == 0 && m5 == 0 {
		fmt.Printf("O número %d é múltiplo de 3 e de 5 ao mesmo tempo.", n1)
	} else {
		fmt.Printf("O número %d não é multiplo de 3 e de 5 ao mesmo tempo.", n1)
	}
}
