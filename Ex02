//Faça um algoritmo que leia um número inteiro e mostre o seu dobro, triplo e quadruplo.

package main

import "fmt"

func main() {
	var n1 int
	fmt.Print("Informe um número: ")
	fmt.Scan(&n1)

	dobro := n1 * 2
	triplo := n1 * 3
	quadruplo := n1 * 4

	fmt.Println("O dobro do número é: ", dobro)
	fmt.Println("O triplo do número é: ", triplo)
	fmt.Print("O quadrplo do número é: ", quadruplo)
}
