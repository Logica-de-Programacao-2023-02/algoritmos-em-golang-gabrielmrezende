Faça um algoritmo que leia o preço de um produto e mostre o seu valor com desconto de 10%.

package main

import "fmt"

func main() {
	var preco float64
	fmt.Print("Qual o valor do produto? R$")
	fmt.Scan(&preco)

	desconto := preco - (preco * 0.1)

	fmt.Print("o valor do produto com desconto será de R$", desconto)
}
