//Faça um algoritmo que leia dois números inteiros e mostre o maior deles.

package main

import "fmt"

func main() {
	var n1, n2 int
	fmt.Print("Infome um número inteiro: ")
	fmt.Scan(&n1)
	fmt.Print("Informe outro número inteiro: ")
	fmt.Scan(&n2)

	if n1 > n2 {
		fmt.Printf("O maior número é %d", n1)
	} else {
		fmt.Printf("O maior número é %d", n2)
	}
}
